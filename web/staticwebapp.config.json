{
  "routes": [
    {
      "route": "/favicon.svg",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/favicon-32.png",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/apple-touch-icon.png",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
